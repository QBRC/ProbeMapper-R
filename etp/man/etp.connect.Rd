## The function is currently defined as
function (url, key) 
{
    if (substr(url, nchar(url), nchar(url)) != "/") {
        url <- paste(url, "/", sep = "")
    }
    DATABASE_API_URL <- url
    API_KEY <- key
    API_KEY_STR <- paste("key=", API_KEY, sep = "")
    return(list(url = url, keyStr = API_KEY_STR))
  }
}
